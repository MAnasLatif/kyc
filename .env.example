# Shufti Pro credentials (get from client)
SHUFTI_CLIENT_ID=xxxxx
SHUFTI_SECRET_KEY=yyyyy

# Public webhook URL (Codespaces se milega, baad me paste karo)
# Example: https://<your-org>-<repo>-<hash>-3000.app.github.dev/kyc/webhook
KYC_WEBHOOK_URL=https://<your-codespace-3000-public>/kyc/webhook

# Optional: after-flow redirect
KYC_REDIRECT_URL=https://example.com/kyc/return

# Cost-safety: max production runs allowed
MAX_IFRAME_RUNS=10

# Server config
PORT=3000
NODE_ENV=development
